{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileUpload": {
    "MaxFileSizeInMB": 10,
    "AllowedExtensions": [ ".pdf", ".doc", ".docx", ".txt", ".jpg", ".jpeg", ".png" ],
    "SecureStoragePath": "App_Data/SecureFiles",
    "TempStoragePath": "App_Data/TempFiles",
    "AllowDirectAccess": false,
    "GenerateUniqueNames": true
  }
}
